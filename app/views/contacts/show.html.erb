<div class="container">
  <div class="row">
    <div class="jumbotron">
       <div class="container">
          <h1>Client information</h1>
       </div>
    </div>
  </div>
  <div class="row">
    <p id="notice"><%= notice %></p>

    <p>
      <strong>First name:</strong>
      <%= @contact.first_name %>
    </p>

    <p>
      <strong>Last name:</strong>
      <%= @contact.last_name %>
    </p>

    <p>
      <strong>Email:</strong>
      <%= @contact.email %>
    </p>

    <%= link_to 'Edit', edit_contact_path(@contact) %> |
    <%= link_to 'Back', contacts_path %>
  </div>
</div>